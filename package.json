{
  "name": "@artsy/lint-changed",
  "version": "5.3.0",
  "description": "Lint only files that have changed since last update",
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "bin": {
    "lint-changed": "./bin/lint-changed.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "index.js",
    "bin/",
    "lib/"
  ],
  "repository": "https://github.com/artsy/lint-changed",
  "author": "Justin Bennett <justin.bennett@artsymail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn compile",
    "type-check": "tsc --pretty --noEmit",
    "compile": "tsc"
  },
  "lint-changed": {
    "*.ts": [
      "echo"
    ]
  },
  "dependencies": {
    "await-to-js": "^2.1.1",
    "commander": "^8.0.0",
    "kleur": "^3.0.3",
    "micromatch": "^4.0.2",
    "p-limit": "^2.2.2"
  },
  "devDependencies": {
    "@artsy/auto-config": "1.2.0",
    "@types/micromatch": "4.0.1",
    "@types/node": "13.7.7",
    "typescript": "5.0.2"
  }
}
